{
    "name": "php-pp/core-contract",
    "description": "Contracts for php-pp/core",
    "type": "library",
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "PhpPp\\Core\\Contract\\": "src/"
        }
    },
    "require": {
        "php": "^7.4||^8.0"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "rm-composer-lock": "rm composer.lock",
        "post-install-cmd": [
            "@rm-composer-lock"
        ],
        "post-update-cmd": [
            "@rm-composer-lock"
        ]
    }
}
